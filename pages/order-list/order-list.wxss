.weui-tab {
    position: relative;
    height: 100%;
}

.weui-navbar {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    position: absolute;
    z-index: 500;
    top: 0;
    width: 100%;
    border-bottom: 1rpx solid #ccc;
    background: #fff;
}

.weui-navbar__item.weui-bar__item_on {
    color: #1aad19;
}

.weui-tab__panel {
    box-sizing: border-box;
    height: 100%;
    padding-top: 50px;
    overflow: auto;
    -webkit-overflow-scrolling: touch;
}

.weui-navbar__slider {
    position: absolute;
    content: " ";
    left: 0;
    bottom: 0;
    width: 6em;
    height: 3px;
    background-color: #1aad19;
    -webkit-transition: -webkit-transform 0.3s;
    transition: -webkit-transform 0.3s;
    transition: transform 0.3s;
    transition: transform 0.3s, -webkit-transform 0.3s;
}

.weui-tab__content {
    /* padding-left: 26rpx;
    padding-right: 26rpx; */
    /* padding-top: 60px; */
    /* text-align: center; */
}

.weui-navbar__item {
    position: relative;
    display: block;
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    padding: 13px 0;
    text-align: center;
    font-size: 0;
}

.weui-navbar__title {
    display: inline-block;
    font-size: 15px;
    max-width: 8em;
    width: auto;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-wrap: normal;
}

.order-list{
    padding-bottom: 30px;
    padding-left: 26rpx;
    padding-right: 26rpx;
    /* border-bottom: 1px solid #ddd; */
}
.order-title{
    padding: 20rpx 0 20rpx 0;
    border-bottom: 1px solid #ddd;
}
.order-title .title-between{
    display: flex;
    justify-content: space-between;
    font-size: 38rpx;
    padding-bottom: 10rpx;
}
.order-code{
    overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
}
.order-food-list .food-between{
    display: flex;
    justify-content: flex-start;
    padding: 10rpx 0 10rpx 0;
}
.order-food-summary .food-between{
    color: #666;
    font-size: 16px;
    display: flex;
    justify-content: flex-end;
}
.food-name{
    margin-right: 100rpx;
}
.operation-list{
    border-top: 1px solid #ddd;
    display: flex;
    justify-content: flex-end;
    padding: 20rpx 10rpx 0rpx 10rpx;
}
.line{
    height: 5px;
    background: #ddd;
    width: 100%;
}
.operation-list view{
    margin-left: 10px;
    width: 220rpx;
    height: 80rpx;
    text-align: center;
    line-height: 80rpx;
    border: 1px solid #ddd;
}
.cancel-order{
    background: #ddd;
    color: #fff;
}
.evaluate{
    background: #FFD700;
    color: rgb(31, 29, 29);
}
.go-pay{
    background: #008B00;
    color: #fff;
}
.take-meal{
    background: #fff;
    color: rgb(31, 29, 29);
}
.bottom-more{
    height: 50px;
    line-height: 50px;
    width: 100%;
    text-align: center;
}
.loading{
  padding: 10rpx;
  text-align: center;
}
.loading:before{
  display: inline-block;
  margin-right: 5rpx;
  vertical-align: middle;
  content: '';
  width: 40rpx;
  height: 40rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: contain;
  animation: rotate 1s linear infinite;
}