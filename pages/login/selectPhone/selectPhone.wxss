}
.header {
  font-size: 36rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: #999999;
  padding-right: 20rpx;
  line-height: 100rpx;
  flex-shrink: 0;
}

.line {
  width: 2rpx;
  height: 28rpx;
  background: rgba(216, 216, 216, 1);
  margin-top: 36rpx;
  margin-right: 20rpx;
  flex-shrink: 0;
}
.des {
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  line-height: 40rpx;
  margin-top: 20rpx;
}

.button-active {
  background: linear-gradient(270deg, #f79c4c 0%, #f7cd4e 99%);
  border-radius: 10rpx;
  font-family: PingFangSC-Regular;
  font-size: 34rpx;
  color: #ffffff;
  letter-spacing: 0;
  text-align: center;
  height: 96rpx;
  width: 100%;
  margin-top: 60rpx;
  line-height: 96rpx;
}

.button-inactive {
  background: #dddddd;
  border-radius: 10rpx;
  font-family: PingFangSC-Regular;
  font-size: 34rpx;
  color: #ffffff;
  letter-spacing: 0;
  text-align: center;
  height: 96rpx;
  width: 100%;
  margin-top: 60rpx;
  line-height: 96rpx;
}
.gouxuan-wrapper {
