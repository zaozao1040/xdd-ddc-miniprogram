.bg-wrapper {
  background-color: #fff;
  padding: 60rpx;
}

.title {
  display: flex;
  margin-top: 100rpx;
  margin-bottom: 40rpx;
  height: 48rpx;
  line-height: 48rpx;
  font-family: PingFangSC-Regular;
  font-size: 35rpx;
  color: #ccc;
}
.titleBtn {
  flex: 1;
  text-align: center;
}
.titleBtnActive {
  color: #222;
  font-size: 40rpx;
}

.section-3 {
  display: flex;
  border-bottom: 1px solid #ebebeb;
  justify-content: space-between;
}

.rg-section-v5 {
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #999999;
  letter-spacing: 0;
  text-align: center;
  background: #f5f5f5;
  border-radius: 28rpx;
  width: 160rpx;
  height: 56rpx;
  line-height: 56rpx;
  margin-top: 22rpx;
  flex-shrink: 0;
}

.rg-section-v6 {
  background: rgba(242, 153, 74, 0.2);
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #f2994a;
  letter-spacing: 0;
  text-align: center;
  border-radius: 28rpx;
  width: 160rpx;
  height: 56rpx;
  line-height: 56rpx;
  margin-top: 22rpx;
  flex-shrink: 0;
}

.placeholder-class {
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  color: #999999;
  letter-spacing: 0;
  text-align: left;
  font-weight: 400;
}

.phone-number {
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  color: #333333;
  letter-spacing: 0;
  text-align: left;
  height: 100rpx;
  line-height: 100rpx;
