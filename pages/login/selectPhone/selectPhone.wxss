.bg-wrapper {
  background-color: #eee;
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
}

.img-wrapper {
  display: flex;
  justify-content: center;
  margin-top: 119rpx;
}

.title {
  text-align: center;
  margin-top: 20.6rpx;
  margin-bottom: 146rpx;
  height: 48rpx;
  line-height: 48rpx;
  font-family: PingFangSC-Regular;
  font-size: 17px;
  color: #1A1A1A;
}

.button {
  background-image: linear-gradient(270deg, #F79C4C 0%, #F7CD4E 99%);
  border-radius: 50rpx;
  margin: 0rpx 62rpx 40rpx 62rpx;
  height: 96rpx;
  text-align: center;
  line-height: 96rpx;
   color: #FFF;

}
.button-phone{
  border-radius: 50rpx;
  margin: 0 62rpx 0 62rpx;
  height: 96rpx;
  text-align: center;
  line-height: 96rpx;
  border: 1px solid #DBDBDB;
  font-family: PingFangSC-Regular;
font-size: 17px;
color: #999999;
}

.button-space {
  margin: 20rpx;
  height: 100rpx;
  text-align: center;
  background-color: #fff;
  border-radius: 15rpx;
  line-height: 100rpx;
  color: #222;
  font-weight: bold;
  font-size: 36rpx;
}

.des {
