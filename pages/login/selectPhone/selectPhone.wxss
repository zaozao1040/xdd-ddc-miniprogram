  line-height: 36rpx;
}

.unchecked-b {
  width: 32rpx;
  height: 32rpx;
  background: rgba(245, 245, 245, 1);
  border-radius: 6rpx;
  border: 1rpx solid rgba(204, 204, 204, 1);
}

.checked-b {
  width: 36rpx;
