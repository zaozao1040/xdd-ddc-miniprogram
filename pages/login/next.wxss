.wrp {
  padding-top: 100rpx;
}
.line1 {
  position: relative;
  height: 88rpx;
  display: flex;
  margin: 40rpx;
}
.input-wrp {
  flex: 1;
  position: relative;
  height: 88rpx;
  display: flex;
  border: 1rpx solid #e6e5e5;
  border-radius: 6rpx;
  background-color: #fff;
  align-items: center;
}
.search-btn {
  background: #f79c4c;
  color: #fff;
  margin: 10rpx 0 10rpx 10rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 10rpx;
  font-size: 25rpx;
  width: 100rpx;
}
.org-name {
  flex: 1;
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #333333;
  letter-spacing: 0;
  text-align: left;
  padding-left: 20rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-break: break-all;
}
.active-btn {
  font-family: PingFangSC-Regular;
  margin: 40rpx;
  height: 96rpx;
  text-align: center;
  border-radius: 10rpx;
  line-height: 96rpx;
  color: #fff;
  font-size: 34rpx;
  background-image: linear-gradient(270deg, #f79c4c 0%, #f7cd4e 99%);
}

.active-btn::after {
  border: none;
}
.inactive-btn {
  font-family: PingFangSC-Regular;
  margin: 40rpx;
  height: 96rpx;
  text-align: center;
  border-radius: 10rpx;
  line-height: 96rpx;
  color: #666;
  font-size: 34rpx;
  background-image: white;
  border: 1rpx solid #ccc;
}

/* 企业列表 */
.org-wrp {
  border: 1rpx solid #f2f1f1;
  border-radius: 6rpx;
  background-color: #fff;
  margin: 0 40rpx 40rpx 40rpx;
}
.item {
  height: 100rpx;
  color: #222222;
  border-bottom: 1rpx solid #e6e5e5;
  padding-left: 20rpx;
}
.name {
  height: 60rpx;
  line-height: 80rpx;
  font-size: 25rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-break: break-all;
}
.nickname {
  height: 40rpx;
  line-height: 20rpx;
  color: #666666;
  font-size: 23rpx;
}
.item:last-child {
  border-bottom: none;
}
