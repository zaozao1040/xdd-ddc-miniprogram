 .wrapper {
     position: fixed;
     top: 0;
     bottom: 0;
     left: 0;
     right: 0;
     /*   padding: 20rpx; */
     background: #eee;
 }
 /* 弹出层 选择优惠券 */
 
 .discountBg-wrapper {
     position: fixed;
     left: 0;
     right: 0;
     top: 0;
     bottom: 0;
     z-index: 99;
     background: #eee;
     overflow: hidden;
 }
 
 .discountBg-headder {
     display: flex;
     justify-content: space-between;
     font-size: 35rpx;
     color: #666;
     padding-left: 20rpx;
     height: 60rpx;
     line-height: 80rpx;
 }
 
 .discountBg-headder-right {
     padding-right: 70rpx;
 }
 
 .discountBg-headder-right-img {
     width: 40rpx;
     height: 40rpx;
     position: fixed;
     right: 20rpx;
     top: 20rpx;
 }
 
 .discountBg-content {
     padding: 0 20rpx 20rpx 20rpx;
 }
 /* 弹出层 填写取餐人 送餐地址 */
 
 .modulebox-background-wrapper {
     position: fixed;
     left: 0;
     right: 0;
     top: 0;
     bottom: 0;
     z-index: 99;
     background: rgba(13, 21, 31, 0.5);
     margin: auto;
     overflow: hidden;
 }
 
 .modulebox-wrapper {
     position: absolute;
     top: 0;
     bottom: 0;
     left: 0;
     right: 0;
     margin: auto;
     height: 380rpx;
     width: 80%;
     padding: 10rpx 20rpx 20rpx 20rpx;
     background: #F5F5F5;
     border-radius: 10rpx;
 }
 
 .modulebox-img-wrapper {
     padding: 60rpx 50rpx;
     box-sizing: border-box;
     width: 100%;
     height: 200rpx;
 }
 
 .modulebox-img {
     width: 100%;
 }
 
 .selectPayType_button {
     border: 2rpx solid #CCCCCC;
     border-radius: 50rpx;
     width: 216rpx;
     height: 68rpx;
     line-height: 68rpx;
     font-family: PingFangSC-Regular;
     font-size: 30rpx;
     color: #333333;
     letter-spacing: 0;
     text-align: center;
 }
 
 .selectPayType_button_check {
     background: linear-gradient(270deg, #F79C4C 0%, #F7CD4E 99%);
     border-radius: 50rpx;
     width: 220rpx;
     height: 72rpx;
     line-height: 72rpx;
     font-family: PingFangSC-Regular;
     font-size: 30rpx;
     color: #FFFFFF;
     letter-spacing: 0;
     text-align: center;
 }
 
 .modulebox-des {
     text-align: center;
     color: #666;
     font-weight: 500;
     font-size: 30rpx;
     margin-top: 40rpx;
 }
 
 .modulebox-button-wrapper {
     position: absolute;
     bottom: 20rpx;
     left: 20rpx;
     right: 20rpx;
     height: 80rpx;
     text-align: center;
     border-radius: 15rpx;
     line-height: 80rpx;
     color: #FFF;
     font-weight: bold;
     font-size: 36rpx;
     display: flex;
     justify-content: space-around;
 }
 
 .modulebox-button-quxiao {
     flex: 1;
     margin: 0 30rpx;
 }
 
 .modulebox-button-queding {
     flex: 1;
     margin: 0 30rpx;
     background: #e79b37;
     color: #FFF;
 }
 
 .section-3 {
     display: flex;
     border-radius: 6rpx;
     height: 100rpx;
     position: relative;
 }
 
 .section-3-cannotChange {
     background: #eee;
 }
 
 .section-3-empty {
     border: 1rpx solid #F56C6C;
 }
 
 .section-3-1 {
     padding: 22rpx 36rpx 24rpx;
     border-top-left-radius: 6rpx;
     border-bottom-left-radius: 6rpx;
     background: #FAFAFA;
 }
 
 .section-3-2 {
     font-family: PingFangSC-Regular;
     font-size: 30rpx;
     color: #333333;
     letter-spacing: 0;
     text-align: left;
     flex: 1;
     background: #fff;
     height: 100rpx;
     line-height: 100rpx;
     padding-left: 20rpx;
 }
 /* 默认地址 */
 
 .address-wrapper {
     display: flex;
     background: linear-gradient(-270deg, #F79C4C 0%, #F7CD4E 99%);
     padding: 10rpx 20rpx;
 }
 
 .address-line1 {
     display: flex;
     font-family: PingFang-SC-Medium;
     font-size: 30rpx;
     color: #FFFFFF;
     letter-spacing: 0;
     line-height: 42rpx;
     margin-bottom: 30rpx;
 }
 
 .address-line2 {
     font-family: PingFang-SC-Medium;
     font-size: 40rpx;
     color: #FFFFFF;
     letter-spacing: 0;
     text-align: left;
     overflow: hidden;
     text-overflow: ellipsis;
     white-space: nowrap;
     line-height: 56rpx;
     margin-bottom: 10rpx;
 }
