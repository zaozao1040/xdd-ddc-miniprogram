 /* 合计 */
 
 .total-wrapper {
     position: sticky;
     bottom: 0;
     background: #fff;
 }
 /* 优惠券 */
 
 .discount-wrapper {
     display: flex;
     justify-content: space-between;
     padding: 10rpx 30rpx
 }
 
 .discount-left {
     display: flex;
     font-size: 30rpx;
     color: #666;
     font-weight: 500;
 }
 
