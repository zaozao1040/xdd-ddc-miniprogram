.in {
  animation-name: show;
  animation-duration: 0.3s;
  animation-timing-function: linear;
  animation-fill-mode: forwards;
}

.out {
  animation-name: fade;
  animation-duration: 0.3s;
  animation-timing-function: linear;
  animation-fill-mode: forwards;
}

@keyframes show {
  from {
    transform: translate(0, 100%);
  }
  to {
    transform: translate(0, 0);
  }
}

@keyframes fade {
  from {
    transform: translate(0, 0);
  }
  to {
    transform: translate(0, 100%);
  }
}

.box-wrapper {
  height: 126rpx;
  width: 126rpx;
  position: absolute;
  left: 28rpx;
  bottom: 0;
}

.cartShake {
  animation-name: cartScale;
  animation-duration: 0.5s;
  animation-timing-function: ease;
  animation-fill-mode: forwards;
  height: 126rpx;
  width: 126rpx;
  position: absolute;
  left: 28rpx;
  bottom: 0;
}

@keyframes cartScale {
  0% {
    transform: scale(1);
  }
  20% {
    transform: scale(0.9);
  }
  40% {
    transform: scale(0.75);
  }
  60% {
    transform: scale(1);
  }
  80% {
    transform: scale(0.8);
  }
  100% {
    transform: scale(1);
  }
}

