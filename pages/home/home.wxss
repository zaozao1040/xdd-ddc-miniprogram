/* 三个标签 */

.label-wrapper {
    display: flex;
    margin: 60rpx 80rpx 80rpx;
    justify-content: space-between;
}

.labelItem {
    flex: 1;
    margin: 10rpx auto;
}

.iconLabel {
    font-size: 60rpx;
    color: #f2993c;
}

.labelDes {
    font-family: PingFang-SC-Regular;
    font-size: 14px;
    color: #555555;
    letter-spacing: 0;
    line-height: 40rpx;
}


/* 提示 */

.reminder-wrapper {
    position: absolute;
    height: 500rpx;
    width: 500rpx;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
}


/* 用户授权框 弹出 */

.modulebox-background-wrapper {
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    z-index: 99;
    background: rgba(13, 21, 31, 0.5);
    margin: auto;
}

.modulebox-wrapper {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    height: 400rpx;
    width: 60%;
    padding: 20rpx;
    background: #fff;
    border-radius: 10rpx;
}

.modulebox-background-wrapper-notice {
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    z-index: 99;
    background: rgba(13, 21, 31, 0.5);
    margin: auto;
}

.modulebox-wrapper-notice {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
    height: 550rpx;
    padding: 20rpx;
    width: 60%;
    border-radius: 10rpx;
    background: #fff;
}

.modulebox-notice-wrapper {
    padding: 6rpx 5rpx;
    box-sizing: border-box;
    width: 100%;
    height: 450rpx;
}

.modulebox-img-wrapper {
    padding: 60rpx 50rpx;
    box-sizing: border-box;
    width: 100%;
    height: 200rpx;
}

.modulebox-img {
    width: 100%;
}

.modulebox-des {
    text-align: center;
    color: #666;
    font-weight: 500;
    font-size: 35rpx;
    margin-top: 50rpx;
}

.modulebox-button {
    position: absolute;
    bottom: 20rpx;
    left: 40rpx;
    right: 40rpx;
    height: 80rpx;
    text-align: center;
    background-color: #e79b37;
    border-radius: 15rpx;
    line-height: 80rpx;
    color: #FFF;
    font-weight: bold;
    font-size: 36rpx;
}


/* 首页提示取餐 */

.homeOrder-wrapper {
    /*   padding: 10rpx;
  background: #eee; */
    border: 1rpx solid #eee;
    border-radius: 10rpx;
    box-shadow: 0rpx 5rpx 5px #aaa;
    margin-bottom: 10rpx;
}

.homeOrder-item {
    display: flex;
    background: #fff;
    border-bottom: #eee solid 1rpx;
}

.homeOrder-item:last-child {
    border-bottom: none;
}

.homeOrder-left-wrapper {
    position: relative;
    width: 100rpx;
    height: 0;
    padding-bottom: 100rpx;
}

.homeOrder-img {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    border-top-left-radius: 10rpx;
    border-bottom-left-radius: 10rpx;
}

.homeOrder-middle-wrapper {
    flex: 1;
}

.homeOrder-middle-line1 {
    padding: 0 10rpx 0 10rpx;
    height: 50rpx;
    line-height: 60rpx;
    display: flex;
    justify-content: space-between;
}

.homeOrder-middle-line1-foodName {
    font-size: 30rpx;
    color: #222;
    font-weight: 500;
}

.icon-zao {
    color: #537c00;
}

.icon-wu1 {
    color: #805300;
}

.icon-night {
    color: #1e90ff;
}

.icon-shenye {
    color: #483d8b;
}

.homeOrder-middle-line1-cabinet {
    font-size: 30rpx;
    color: #222;
    font-weight: 500;
    color: #43964e;
}

.homeOrder-middle-line2 {
    padding: 0 10rpx 0 10rpx;
    height: 50rpx;
    line-height: 60rpx;
    display: flex;
    justify-content: space-between;
    font-size: 25rpx;
    color: #666;
    font-weight: 0;
}

.homeOrder-middle-line2-cabinet {
    height: 60rpx;
    line-height: 60rpx;
    font-size: 30rpx;
    color: #fff;
}

.homeOrder-middle-line3-cabinet {
    line-height: 30rpx;
    font-size: 20rpx;
    color: #fff;
    width: 100%;
    text-align: center;
}

.homeOrder-middle-line2-des {
    font-size: 20rpx;
    color: #43964e;
    background: #fff;
    padding: 0 20rpx;
    border-radius: 20rpx;
}

.homeOrder-middle-line2-des-nocabinet {
    margin: 10rpx;
    width: 80rpx;
    height: 80rpx;
    border-radius: 50rpx;
    background: #43964e;
    text-align: center;
    line-height: 80rpx;
    font-size: 30rpx;
    color: #fff;
    font-weight: 500;
}

.homeOrder-right-wrapper {
    /* width: 100rpx; */
    height: 100rpx;
    background: #43964e;
    text-align: center;
    font-size: 30rpx;
    color: #fff;
    font-weight: 500;
    border-radius: 10rpx;
    padding: 10rpx 10rpx 0;
}


/* 轮播图两种样式 */

.imageBanner_active {
    width: 100%;
    height: 100%;
    border-top-left-radius: 15rpx;
    border-top-right-radius: 15rpx;
}

.imageBanner_small {
    width: 95%;
    height: 95%;
    margin-left: 20rpx;
    margin-right: 20rpx;
    position: absolute;
    bottom: 0;
    border-top-left-radius: 8rpx;
    border-top-right-radius: 8rpx;
}