/* -------首页取餐 */

.takefood-title {
    font-family: PingFang-SC-Medium;
    font-size: 36rpx;
    color: #333333;
    letter-spacing: 0;
    text-align: left;
    margin-left: 6rpx;
    margin-bottom: 20rpx;
    line-height: 50rpx;
}

.takefood-food {
    background: #FFFFFF;
    border: 1rpx solid #EBEBEB;
    box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.08);
    border-radius: 4px;
    width: 676rpx;
    height: 168rpx;
    display: flex;
    margin-right: 12rpx;
}

.takefood-img {
    width: 120rpx;
    height: 120rpx;
    border-radius: 8rpx;
    margin: 24rpx 10rpx 24rpx 24rpx;
}

.takefood-img-wrapper {
    flex-shrink: 0;
}

.takefood-cabinet {
    margin-top: 16rpx;
    flex-grow: 1;
}

.takefood-cabinet-number {
    font-family: PingFang-SC-Medium;
    font-size: 18px;
    color: #F79C4C;
    letter-spacing: 0;
    text-align: left;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    width: 270rpx;
}

.takefood-cabinet-title {
    font-family: PingFang-SC-Regular;
    font-size: 24rpx;
    color: #333333;
    letter-spacing: 0;
    text-align: left;
    margin-right: 20rpx;
    white-space: nowrap;
}

.takefood-button {
    background-image: linear-gradient(-90deg, #F79C4C 0%, #F7CD4E 99%);
    border-radius: 34rpx;
    width: 120rpx;
    height: 68rpx;
    line-height: 68rpx;
    font-family: PingFang-SC-Medium;
    font-size: 26rpx;
    color: #FFFFFF;
    letter-spacing: 0;
    text-align: center;
    float: right;
    margin: 50rpx 24rpx 0 0;
    flex-shrink: 0;
}

.takefood-mealType {
    /* width: 56rpx; */
    font-family: PingFang-SC-Medium;
    font-size: 28rpx;
    color: #333333;
    letter-spacing: 0;
    text-align: left;
    margin-right: 20rpx;
    white-space: nowrap;
}

.takefood-foodname {
    width: 272rpx;
    font-family: PingFang-SC-Medium;
    font-size: 28rpx;
    color: #333333;
    letter-spacing: 0;
    text-align: left;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.takefood-foodcount {
    font-family: PingFang-SC-Medium;
    font-size: 28rpx;
    color: #333333;
    letter-spacing: 0;
    text-align: left;
}

.takefood-time {
    font-family: PingFang-SC-Regular;
    font-size: 24rpx;
    color: #999999;
    letter-spacing: 0;
    text-align: left;
    line-height: 34rpx;
}

.appointment-remind {
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #999999;
    letter-spacing: 0;
    text-align: left;
    margin-top: 66rpx;
}

.appointment-title {
    font-family: PingFang-SC-Medium;
    font-size: 36rpx;
    color: #333333;
    letter-spacing: 0;
    text-align: left;
    line-height: 50rpx;
    margin-top: 60rpx;
    margin-left: 60rpx;
    margin-bottom: 20rpx;
}

.appointment-choice-list {
    margin-left: 60rpx;
    margin-bottom: 80rpx;
}

.appointment-choice-right {
    font-family: PingFang-SC-Regular;
    font-size: 30rpx;
    color: #AAAAAA;
    letter-spacing: 0;
    text-align: left;
    line-height: 88rpx;
    margin-left: 10rpx;
}

.appointment-choice-right-checked {
    font-family: PingFang-SC-Regular;
    font-size: 30rpx;
    color: #F2994A;
    letter-spacing: 0;
    text-align: left;
    line-height: 88rpx;
    margin-left: 10rpx;
}

.appointment-choice-foodtype-title {
    font-family: PingFang-SC-Medium;
    font-size: 36rpx;
    color: #333333;
