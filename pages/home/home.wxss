    padding-left: 60rpx;
    padding-bottom: 20rpx;
    line-height: 50rpx;
}

.appointment-choice-foodtype {
    padding: 9rpx 10rpx 10rpx 60rpx;
    display: flex;
    border-top: 1rpx solid #EBEBEB;
}

.appointment-choice-foodtype-name {
    font-family: PingFang-SC-Medium;
    font-size: 32rpx;
    color: #333333;
    letter-spacing: 0;
    text-align: left;
    line-height: 44rpx;
    margin-top: 14rpx;
}

.appointment-choice-foodtype-deadline {
    font-family: PingFang-SC-Regular;
    font-size: 28rpx;
    color: #AAAAAA;
    letter-spacing: 0;
    text-align: left;
    margin-top: 10rpx;
    line-height: 40rpx;
    display: flex;
}

.mask {
    z-index: 100;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.5);
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}


