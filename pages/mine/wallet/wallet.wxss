 .selection {
     font-family: PingFangSC-Regular;
     font-size: 24rpx;
     color: #FFFFFF;
     letter-spacing: 0;
     text-align: right;
     padding-right: 20rpx;
 }
 
 .header-wrapper {
     background: linear-gradient(270deg, #F79C4C 0%, #F7BE4C 99%);
     border-radius: 12rpx;
     margin-bottom: 20rpx;
     text-align: center;
     padding: 20rpx 0 48rpx;
 }
 
 .header-des {
     font-family: PingFangSC;
     font-size: 20rpx;
     color: #FFFFFF;
     letter-spacing: 0;
     text-align: center;
     margin: 8rpx 0;
     line-height: 28rpx;
 }
 
 .header-balance {
     font-family: DINAlternate-Bold;
     font-size: 50rpx;
     color: #FFFFFF;
     letter-spacing: 0;
     text-align: center;
     line-height: 58rpx;
 }
 
 .content-title {
     font-family: PingFangSC;
     font-size: 34rpx;
     color: #333333;
     letter-spacing: 0;
     text-align: left;
     margin-bottom: 30rpx;
 }
 
 .item-icon {
     font-family: PingFangSC-Regular;
     font-size: 36rpx;
     letter-spacing: 0;
     line-height: 50rpx;
     text-align: center;
 }
 
 .item-money {
     font-family: PingFangSC-Regular;
     font-size: 24rpx;
     letter-spacing: 0;
     text-align: center;
     line-height: 30rpx;
 }
 
 .recharge-title {
     font-family: PingFangSC-Regular;
     font-size: 34rpx;
     color: #333333;
     letter-spacing: 0;
     text-align: left;
     margin: 22rpx 0 30rpx;
 }
 
 .description {
     font-family: PingFangSC-Regular;
     font-size: 24rpx;
     color: #999999;
     letter-spacing: 0;
     text-align: left;
     padding-top: 20rpx;
 }
 
 .wechat {
     font-family: PingFangSC-Regular;
     font-size: 28rpx;
     color: #333333;
     letter-spacing: 0;
     text-align: left;
     margin-left: 20rpx;
     flex-grow: 1;
     line-height: 54rpx;
 }
 
 .chongzhi-item {
     background: #F9F9F9;
     border-radius: 16rpx;
     width: 200rpx;
     padding: 10rpx 0;
 }
 
 .chongzhi-item-active {
     background: #F2994A;
     border-radius: 16rpx;
     width: 200rpx;
     padding: 10rpx 0;
 }
 
 .c_buttonPosition_forCalculate {
     padding: 0 20rpx 20rpx 20rpx;
     position: fixed;
     bottom: 0;
     left: 0;
     right: 0;
     z-index: 10;
 }
 
 .button {
     background: linear-gradient(270deg, #F79C4C 0%, #F7CD4E 99%);
     border-radius: 44rpx;
     font-family: PingFangSC-Regular;
     font-size: 30rpx;
     color: #FFFFFF;
     letter-spacing: 0;
     text-align: center;
     line-height: 88rpx;
     height: 88rpx;
 }
 
 .content-header {
     display: flex;
     height: 104rpx;
     line-height: 104rpx;
     background: #F5F5F5;
     padding: 0 30rpx;
     justify-content: space-between;
 }
 
 .content-header-left {
     font-family: PingFangSC;
     font-size: 34rpx;
     color: #333333;
     letter-spacing: 0;
 }
 
 .content-header-right {
     font-family: PingFangSC;
     font-size: 24rpx;
     color: #999999;
     letter-spacing: 0;
 }
 
 .content-time {
     background: #FFFFFF;
     display: flex;
     height: 100rpx;
     padding: 0 30rpx;
     justify-content: space-between;
 }
 
 .content-time-left {
     margin: 32rpx 0;
     width: 8rpx;
     height: 36rpx;
     background: #F79C4C;
 }
 
 .content-time-right {
     font-family: PingFangSC-Regular;
     font-size: 28rpx;
     color: #333333;
     letter-spacing: 0;
     text-align: left;
     line-height: 100rpx;
 }
 /* 积分记录的内容区域 */
 
 .integral-item {
     border-top: 2rpx solid #F0F0F0;
     padding: 0 30rpx;
     display: flex;
     justify-content: space-between;
 }
 
 .integral-time {
     font-family: PingFangSC;
     font-size: 24rpx;
     color: #999999;
     letter-spacing: 0;
     text-align: left;
     padding: 10rpx 0 14rpx;
     line-height: 34rpx;
 }
 
 .integral-type {
     font-family: PingFangSC;
     font-size: 30rpx;
     color: #333333;
     letter-spacing: 0;
     text-align: left;
     padding-top: 16rpx;
     line-height: 42rpx;
 }
 
 .Integral-value-add {
     font-family: PingFangSC;
     font-size: 34rpx;
     color: #F79C4C;
     letter-spacing: 0;
     text-align: right;
     padding-top: 36rpx;
     line-height: 48rpx;
 }
 
 .Integral-value-minus {
     font-family: PingFangSC;
     font-size: 34rpx;
     color: #333333;
     letter-spacing: 0;
     text-align: right;
     padding-top: 36rpx;
     line-height: 48rpx;
 }
 
 .noResult {
     padding: 10rpx 20rpx;
     font-size: 40rpx;
     color: #666;
 }
 
 .money-list {
     display: flex;
     justify-content: space-between;
     margin-bottom: 20rpx;
 }
 
 .recharge-way {
     display: flex;
     padding-bottom: 30rpx;
     border-bottom: 1rpx solid #EBEBEB;
 }