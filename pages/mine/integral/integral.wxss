 /********** 头部 **********/
 
 .header-title-left {
     font-family: PingFangSC;
     font-size: 24rpx;
     color: #333;
     letter-spacing: 0;
     text-align: left;
     line-height: 34rpx;
     padding: 0 4rpx 0 298rpx;
 }
 
 .header-title-right {
     font-family: DINAlternate-Bold;
     font-size: 18rpx;
     color: #FFFFFF;
     letter-spacing: 0;
     text-align: center;
     background: #DDDDDD;
     border-radius: 12rpx;
     width: 24rpx;
     height: 24rpx;
     line-height: 24rpx;
     margin-top: 5rpx;
 }
 
 .header-bottom-top {
     font-family: PingFangSC-Regular;
     font-size: 28rpx;
     color: #FFFFFF;
     letter-spacing: 0;
     line-height: 40rpx;
 }
 
 .header-bottom-bottom {
     font-family: PingFangSC-Regular;
     font-size: 20rpx;
     color: #FFFFFF;
     letter-spacing: 0;
     line-height: 28rpx;
 }
 
 .header-wrapper {
     background: linear-gradient(270deg, #F79C4C 0%, #F7BE4C 99%);
     padding-top: 20rpx;
 }
 
 .header {
     padding: 30rpx 0 26rpx;
     background: #FFFFFF;
     border-radius: 12rpx;
     margin: 0 30rpx 20rpx;
 }
 
 .header-item {
     font-family: DINAlternate-Bold;
     font-size: 50rpx;
     color: #333;
     letter-spacing: 0;
     text-align: center;
     line-height: 58rpx;
 }
 /********** 内容 **********/
 /* 标题 */
 
 .content-header {
     display: flex;
     height: 104rpx;
     line-height: 104rpx;
     background: #F5F5F5;
     padding: 0 30rpx;
     justify-content: space-between;
 }
 
 .content-header-left {
     font-family: PingFangSC;
     font-size: 34rpx;
     color: #333333;
     letter-spacing: 0;
 }
 
 .content-header-right {
     font-family: PingFangSC;
     font-size: 24rpx;
     color: #999999;
     letter-spacing: 0;
 }
 
 .content-time {
     background: #FFFFFF;
     display: flex;
     height: 100rpx;
     padding: 0 30rpx;
     /* justify-content: space-between; */
 }
 
 .content-time-left {
     margin: 32rpx 0;
     width: 8rpx;
     height: 36rpx;
     background: #F79C4C;
 }
 
 .content-time-right {
     font-family: PingFangSC-Regular;
     font-size: 28rpx;
     color: #333333;
     letter-spacing: 0;
     text-align: left;
     line-height: 100rpx;
 }
 /* 积分记录的内容区域 */
 
 .integral-item {
     border-top: 2rpx solid #F0F0F0;
     padding: 0 30rpx;
     display: flex;
     justify-content: space-between;
 }
 
 .integral-time {
     font-family: PingFangSC;
     font-size: 24rpx;
     color: #999999;
     letter-spacing: 0;
     text-align: left;
     padding: 10rpx 0 14rpx;
     line-height: 34rpx;
 }
 
 .integral-type {
     font-family: PingFangSC;
     font-size: 30rpx;
     color: #333333;
     letter-spacing: 0;
     text-align: left;
     padding-top: 16rpx;
     line-height: 42rpx;
 }
 
 .Integral-value-add {
     font-family: PingFangSC;
     font-size: 34rpx;
     color: #F79C4C;
     letter-spacing: 0;
     text-align: right;
     padding-top: 36rpx;
     line-height: 48rpx;
 }
 
 .Integral-value-minus {
     font-family: PingFangSC;
     font-size: 34rpx;
     color: #333333;
     letter-spacing: 0;
     text-align: right;
     padding-top: 36rpx;
     line-height: 48rpx;
 }
 
 .noResult {
     padding: 10rpx 20rpx;
     font-size: 40rpx;
     color: #666;
 }
 
 .header-desc {
     display: flex;
 }
 
 .header-3 {
     background: rgba(102, 102, 102, 0.1);
     display: flex;
     padding: 16rpx 0;
 }
 
 .header-3-left {
     flex-grow: 1;
     text-align: center;
     width: 360rpx;
 }
 
 .header-3-middle {
     width: 2rpx;
     background: rgba(255, 255, 255, 0.30);
     margin-top: 4rpx;
     height: 60rpx;
 }
 
 .leftleft {
     flex-grow: 1;
     text-align: center;
     width: 340rpx;
     margin: 20rpx 0;
 }
 
 .leftleft-title {
     font-family: Helvetica;
     font-size: 24rpx;
     color: #999999;
     letter-spacing: 0;
     text-align: center;
     line-height: 34rpx;
 }
 
 .leftleft-num {
     font-family: DINAlternate-Bold;
     font-size: 36rpx;
     color: #333333;
     letter-spacing: 0;
     text-align: center;
     line-height: 42rpx;
 }
 
 .middlemiddle {
     margin-top: 30rpx;
     background: #DDDDDD;
     width: 2rpx;
     height: 56rpx;
 }
 
 .header-button {
     background-image: linear-gradient(270deg, #F79C4C 0%, #F7BE4C 99%);
     border-radius: 34rpx;
     width: 240rpx;
     height: 68rpx;
     line-height: 68rpx;
     text-align: center;
     font-family: Helvetica;
     font-size: 30rpx;
     color: #FFFFFF;
     letter-spacing: 0;
     margin: 0 auto;
 }
 
 .dialog-title {
     font-family: PingFangSC-Regular;
     font-size: 36rpx;
     color: #000000;
     text-align: center;
     padding-top: 63rpx;
     line-height: 50rpx;
 }
 
 .dialog-des {
     font-family: PingFangSC-Regular;
     font-size: 30rpx;
     color: #999999;
     text-align: center;
     margin: 19rpx 0 54rpx;
 }
 
 .dialog-button {
     font-family: PingFang-SC-Medium;
     font-size: 36rpx;
     color: #576B95;
     text-align: center;
     padding-top: 25rpx;
     border-top: 1rpx solid #E5E5E5;
 }
 
 .dialog-warpper {
     position: fixed;
     left: 0;
     right: 0;
     bottom: 0;
     top: 0;
     z-index: 99;
     margin: auto;
     background: rgba(0, 0, 0, 0.40);
 }
 
 .dialog {
     z-index: 100;
     position: fixed;
     left: 0;
     right: 0;
     bottom: 0;
     top: 0;
     width: 560rpx;
     height: 329rpx;
     margin: auto;
     background: #fff;
     text-align: center;
     border-radius: 8rpx;
 }
 
 .dialog-button-group {
     margin: 0 30rpx;
     display: flex;
     justify-content: space-between;
 }
 
 .dialog-button-cancel {
     border: 2rpx solid #CCCCCC;
     border-radius: 50rpx;
     height: 76rpx;
     width: 236rpx;
     text-align: center;
     line-height: 76rpx;
     font-family: PingFangSC-Regular;
     font-size: 30rpx;
     color: #333333;
     letter-spacing: 0;
 }
 
 .dialog-button-confirm {
     background: linear-gradient(270deg, #F79C4C 0%, #F7CD4E 99%);
     border-radius: 50rpx;
     width: 240rpx;
     height: 80rpx;
     line-height: 80rpx;
     font-family: PingFangSC-Regular;
     font-size: 30rpx;
     color: #FFFFFF;
     letter-spacing: 0;
     text-align: center;
 }