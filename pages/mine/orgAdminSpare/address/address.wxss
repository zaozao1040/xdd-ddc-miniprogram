.section-3 {
  border-radius: 6rpx;
  margin: 16rpx 16rpx 0 16rpx;
  display: flex;
  background: #ffffff;
}

.phone-number {
  height: 100rpx;
  line-height: 100rpx;
  padding-left: 20rpx;
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #333333;
  letter-spacing: 0;
  text-align: left;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.phone-address {
  display: flex;
  justify-content: space-between;
}

/* 企业选择 */

.address-wrapper {
  background: #f5f5f5;
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
}

.placeholder-class {
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #999999;
  letter-spacing: 0;
  text-align: left;
}

.addressList-wrapper {
  background: #fff;
  margin: 5rpx 16rpx 0 16rpx;
}

.noResult {
  padding: 10rpx 20rpx;
  font-size: 40rpx;
  color: #666;
}

.button {
  margin: 42rpx 16rpx;
  width: 100%;
  background-image: linear-gradient(270deg, #f79c4c 0%, #f7cd4e 99%);
  border-radius: 50rpx;
  font-family: PingFangSC-Regular;
  font-size: 34rpx;
  color: #ffffff;
  letter-spacing: 0;
  text-align: center;
  line-height: 96rpx;
  width: 712rpx;
  height: 96rpx;
}

.address {
  background: #ffffff;
  height: 100rpx;
  flex-grow: 1;
  padding-right: 70rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
