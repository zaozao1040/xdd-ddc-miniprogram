/* 钱包 优惠券 积分 */
.offer-wrapper{
  display: flex;
  background: #fff;
}
.offerItem-wrapper{
  flex:1;
  text-align: center;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
  border-right:1px solid #ddd;
}
.offerItem-wrapper:last-child{
  border:none; 
}
.money-1{
  font-size: 60rpx;
  color: red;
}
.money-2{
  font-size: 20rpx;
  color:red;
}
.youhui-1{
  font-size: 60rpx;
  color: blue;
}
.youhui-2{
  font-size: 20rpx;
  color:blue;
}
.point-1{
  font-size: 60rpx;
  color: goldenrod;
}
.point-2{
  font-size: 20rpx;
  color:goldenrod;
}
.offer-title{
  font-size: 30rpx;
  color:#222;
  font-weight: 400;
}
.iconfont-offertitle{
  font-size: 30rpx;
  color:forestgreen;
  font-weight: 400;
}
/* 地址 浏览记录 下单闹钟 客服等 */
