/* pages/mine/mine.wxss */
.mine-wrapper{
  background: #ddd;
  position: fixed;
  top:0;
  bottom:0;
  left: 0;
  right: 0;
}
.header{
  display: flex;
  background: darkseagreen;
  padding: 60rpx 40rpx;
}
.left{
  width:120rpx;
  height: 120rpx;
  
}
.left-image{
  width: 100%;
  height: 100%;
  border-radius: 60rpx;
}
.middle{
  flex: 1;
  padding-left: 30rpx;
  margin: auto;
}
.userName{
  font-size: 40rpx;
  color: #fff;
  font-weight: 500;
}
.userPhone{
  font-size: 30rpx;
  color: #fff;
}
.right{
  margin: auto;
}
.iconfont-title{
  font-size: 50rpx;
  color: #fff; 
}
