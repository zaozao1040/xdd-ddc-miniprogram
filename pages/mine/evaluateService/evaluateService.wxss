.title{
  padding: 30rpx 0 0 0;
  text-align: center;
  font-family: PingFangSC-Regular;
font-size: 30rpx;
color: #333333;

}
.itemWrapper {
  padding: 30rpx;
  border: 2rpx solid #f5fff5;
}
.itemTitle {
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #333333;
}
.itemContent {

}
.itemContentLine{
  display: flex;
  justify-content: space-between;
}
.lineLeft {
  font-size: 26rpx;
  color: #666666;
  height: 56rpx;
  line-height: 56rpx;
  width: 156rpx;
}

.lineRight {
  display: flex;
  justify-content: center;
  flex-direction: row;
  height: 56rpx;
}
.lineRightDes {
  font-size: 26rpx;
  color: #333333;
  height: 56rpx;
  line-height: 56rpx;
  text-align: left;
  width: 56rpx;
}

.content {
  margin: 40rpx 30rpx;
  padding: 22rpx;
  height: 140rpx;
  border: 2rpx solid #F5F5F5;
  border-radius: 6rpx;
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #CCCCCC;
}
/* 上传图片 */
 
.img-wrapper {
  display: flex;
  padding:0 30rpx;
}
.image-list {
  display: flex;
}
.image {
  margin-right: 10rpx;
  height: 160rpx;
  width: 160rpx;
  color: #999;
  background: #F9F9F9;
  border-radius: 3px;
}

.image-remove {
  line-height: 28rpx;
  text-align: center;
  font-size: 20rpx;
  background: #333;
  width: 30rpx;
  height: 28rpx;
  position: absolute;
  color: #fff;
  border-radius: 50%;
  padding-bottom: 2rpx;
  z-index: 500;
  margin-left: 140rpx;
  margin-top: 10rpx;
}

.image-shangchuan {
  margin-right: 10rpx;
  height: 160rpx;
  width: 160rpx;
}

.yingjifen {
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #F79C4C;
  margin-top: 120rpx;
}
.button {
  background-image: linear-gradient(270deg, #F79C4C 0%, #F7CD4E 99%);
  border-radius: 50rpx;
  font-family: PingFangSC-Regular;
  font-size: 34rpx;
  color: #FFFFFF;
  text-align: center;
  height: 96rpx;
  line-height: 96rpx;
  margin: 12rpx 32rpx;
}