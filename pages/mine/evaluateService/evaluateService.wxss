.title {
    padding: 10rpx 0 0 0;
    text-align: center;
    font-family: PingFangSC-Regular;
    font-size: 30rpx;
    color: #333333;
}

.itemWrapper {
    margin: 32rpx 60rpx 0 30rpx;
    border-bottom: 2rpx solid #f5f5f5;
}

.itemTitle {
    font-family: PingFangSC-Medium;
    font-size: 30rpx;
    color: #333333;
    padding: 30rpx 0 22rpx;
    border-top: 2rpx solid #f5f5f5;
}

.itemContentLine {
    display: flex;
    padding-bottom: 10rpx;
}

.lineLeft {
    font-size: 26rpx;
    color: #666666;
    height: 56rpx;
    line-height: 56rpx;
    width: 174rpx;
}

.lineRight {
    display: flex;
    height: 56rpx;
}

.lineRightDes {
    font-size: 26rpx;
    color: #333333;
    height: 56rpx;
    line-height: 56rpx;
    text-align: left;
    margin-left: -12rpx;
}

.content {
    margin: 40rpx 60rpx 20rpx 30rpx;
    padding: 22rpx;
    height: 140rpx;
    border: 2rpx solid #F5F5F5;
    border-radius: 6rpx;
    font-family: PingFangSC-Regular;
    font-size: 30rpx;
    color: #CCCCCC;
}


/* 上传图片 */

.img-wrapper {
    display: flex;
    padding: 0 30rpx;
    margin-bottom: 200rpx;
}

.image-list {
    display: flex;
}

.image {
    margin-right: 10rpx;
    height: 160rpx;
    width: 160rpx;
    color: #999;
    background: #F9F9F9;
    border-radius: 3px;
}

.image-remove {
    line-height: 28rpx;
    text-align: center;
    font-size: 20rpx;
    background: #333;
    width: 30rpx;
    height: 28rpx;
    position: absolute;
    color: #fff;
    border-radius: 50%;
    padding-bottom: 2rpx;
    z-index: 500;
    margin-left: 140rpx;
    margin-top: 10rpx;
}

.image-shangchuan {
    margin-right: 10rpx;
    height: 160rpx;
    width: 160rpx;
}

.yingjifen {
    font-family: PingFangSC-Regular;
    font-size: 26rpx;
    color: #F79C4C;
    margin-top: 120rpx;
}

.button {
    background-image: linear-gradient(270deg, #F79C4C 0%, #F7CD4E 99%);
    border-radius: 50rpx;
    font-family: PingFangSC-Regular;
    font-size: 34rpx;
    color: #FFFFFF;
    text-align: center;
    height: 96rpx;
    line-height: 96rpx;
    bottom: 12rpx;
    left: 0;
    right: 0;
    width: 688rpx;
    margin: auto;
    position: fixed;
    z-index: 1001;
    opacity: 1;
}