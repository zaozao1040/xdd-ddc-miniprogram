.section-1 {
    width: 100%;
    height: 134rpx;
    margin-top: 20rpx;
    margin-bottom: 10rpx;
}

.section-2 {
    width: 280rpx;
    padding: 0 50rpx;
    margin: 20rpx auto 30rpx;
    font-size: 30rpx;
    text-align: center;
    color: #808080;
    font-family: "微软雅黑", "华文细黑", Arial, Helvetica, sans-serif;
}

.section-2-1 {
    display: inline-block;
    padding: 5rpx 0;
    font-size: 32rpx;
}

.section-3 {
    display: flex;
    border-radius: 6rpx;
    margin-bottom: 20rpx;
}

.section-3-empty {
    border: 1rpx solid #F56C6C;
}

.section-3-1 {
    padding: 22rpx 36rpx 24rpx;
    background: #FAFAFA;
}

.section-3-2 {
    font-family: PingFangSC-Regular;
    font-size: 30rpx;
    color: #999999;
    letter-spacing: 0;
    text-align: left;
    flex: 1;
    background: #fff;
    height: 100rpx;
    line-height: 100rpx;
    padding-left: 20rpx;
}

.section-3-3 {
    width: 96rpx;
    height: 100%;
    border-right: 1rpx solid #bbb;
}

.section-4 {
    width: 100%;
    margin-top: 25rpx;
    height: 100rpx;
    text-align: center;
    background-color: #e79b37;
    border-radius: 15rpx;
    line-height: 100rpx;
    color: #FFF;
    font-weight: bold;
    font-size: 36rpx;
}

.section-5 {
    font-size: 18rpx;
    margin: 13rpx 0;
    display: flex;
    margin-top: 20rpx;
}

.rg-section-v5 {
    border: 1px solid #bfbfbf;
    font-size: 22rpx;
    height: 58rpx;
    line-height: 58rpx;
    width: 138rpx;
    text-align: center;
    margin-right: 28rpx;
    border-radius: 10rpx;
    color: #e79b37;
}

.section-6 {
    display: flex;
    justify-content: flex-end;
    font-size: 18rpx;
    color: #e79b37;
    text-align: right;
    margin: 13rpx 0;
}


/* 企业选择 */

.organize-wrapper {
    background: #F5F5F5;
    padding: 16rpx;
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
}

.search-wrapper {
    background: #FFF;
    margin-top: 10rpx;
}

.inputSearch-wrapper {
    border: #ddd 1rpx solid;
    border-radius: 40rpx;
    padding: 10rpx 20rpx;
    padding-left: 40rpx;
    margin: 10rpx;
}

.inputSearch {
    height: 40rpx;
    line-height: 40rpx;
    font-size: 30rpx;
}

.c_scrollPosition_forCalculate {
    margin-top: 20rpx;
}

.organizeItem-wrapper {
    display: flex;
    justify-content: space-between;
    margin: 10rpx 20rpx;
    border-bottom: 1rpx solid #ddd;
}

.organizeItem-left {}

.organizeName {
    font-size: 40rpx;
    color: #222;
    font-weight: 500;
}

.organizeAddress {
    font-size: 30rpx;
    color: #666;
    font-weight: 400;
}

.organizeItem-right {
    font-size: 30rpx;
    color: #666;
    font-weight: 400;
}


/* 返回 */

.goback {
    position: fixed;
    right: 100rpx;
    bottom: 150rpx;
    width: 100rpx;
    height: 100rpx;
    line-height: 100rpx;
    border-radius: 50rpx;
    background: #e79b37;
    text-align: center;
    font-size: 60rpx;
    color: #fff;
}

.noResult {
    padding: 10rpx 20rpx;
    font-size: 40rpx;
    color: #666;
}

.c_buttonPosition_forCalculate {
    padding: 0 20rpx 20rpx 20rpx;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
}

.button {
    width: 100%;
    margin-top: 25rpx;
    height: 100rpx;
    text-align: center;
    background-color: #e79b37;
    border-radius: 15rpx;
    line-height: 100rpx;
    color: #FFF;
    font-weight: bold;
    font-size: 36rpx;
}