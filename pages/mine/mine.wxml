<view class="mine-wrapper">
    <image src="/images/mine/me_bg.png" style='height:300rpx;width:100%;z-index:-1;position:absolute; '></image>
    <view>
        <view style=' z-index:100;position:absolute; top:20rpx;left:117rpx;' wx:if='{{userInfo.userType=="ORG_ADMIN" &&userInfo.orgAdmin}}'>
            <image src="/images/mine/crown.png" style=' height:40rpx;width:48rpx;transform:rotate(45deg);'></image>
        </view>
        <view class='wrapper-top'>

            <view style='margin:0 10rpx 0 30rpx;width:120rpx;height:120rpx;'>
                <image wx:if="{{userInfo.headImage}}" style='width:120rpx;height:120rpx;border-radius: 50%;' src="{{userInfo.headImage}}"></image>
                <image wx:else style='width:120rpx;height:120rpx;' src="/images/mine/huiyuan.png" alt=""></image>
            </view>
            <view style='margin-top:20rpx;' catchtap='gotoDetailInfo'>
                <view class='name'>
                    <view style="display:inline-block;">{{userInfo.userName}}</view>
                    <view style="display:inline-block;">
                        <image src="/images/mine/xiangyoubaise.png" style='width:48rpx;height:48rpx;position: absolute;top:0;bottom: 0;margin: auto;'></image>
                    </view>
                </view>
                <view wx:if='{{userInfo.organizeName}}'>
                    <view class='address' wx:if='{{userInfo.userType=="ORG_ADMIN"||userInfo.userType=="ADMIN"}}'>
                        {{userInfo.organizeName}}
                    </view>
                    <view class='address' style='width:560rpx;' wx:else>
                        {{userInfo.organizeName}}
                    </view>
                </view>

            </view>
            <view class="org" bindtap="changeRole" wx:if='{{userInfo.userType=="ORG_ADMIN"||userInfo.userType=="ADMIN"}}'>
                <text wx:if="{{userInfo.orgAdmin}}">切换为"企业员工"</text>
                <text wx:elif='{{userInfo.userType=="ORG_ADMIN"}}'>切换为"企业管理员"</text>
                <text wx:elif='{{userInfo.userType=="ADMIN"}}'>切换为"超级管理员"</text>
            </view>
        </view>
        <view wx:if="{{userInfo}}" class='offer-item'>
            <!-- 钱包 优惠券 积分 -->
            <!-- <view class="offerItem-wrapper" bindtap='gotoWallet'>
                <view style='display: flex;padding-left:28rpx;'>
                    <view class='offer-title' style='line-height:42rpx;padding-right:10rpx;'> 个人余额 </view>
                    <view class='offer-num-balance'> {{balance}} </view>

                </view>
                <view style='display: flex;padding-left:28rpx;'>
                    <view class='offer-title' style='line-height:42rpx;padding:0 10rpx 0 0;'> 企业余额 </view>
                    <view class='offer-num-balance'> {{balance}} </view>
                </view>
            </view> -->
            <view class="offerItem-wrapper" bindtap='gotoWallet'>
                <view class='offer-num'> {{balance}} </view>
                <view class='offer-title'> 余额 </view>
            </view>
            <view style='width:2rpx;height:30rpx;margin-top:26rpx;background: rgba(255,255,255,0.5);'></view>
            <!-- <view style="position: relative;">
                <image src="/images/mine/fengexian.png" style='width:1rpx;height:30rpx;position: absolute;top:0;bottom: 0;margin: auto;'></image>
            </view> -->
            <view class="offerItem-wrapper" bindtap='gotoDiscount' >
                <view class='offer-num'> {{discount}} </view>
                <view class='offer-title'> 优惠券 </view>
            </view>
            <view style='width:2rpx;height:30rpx;margin-top:26rpx;background: rgba(255,255,255,0.5);'></view>
            <!-- <view style="position: relative;">
                <image src="/images/mine/fengexian.png" style='width:1rpx;height:30rpx;position: absolute;top:0;bottom: 0;margin: auto;'></image>
            </view> -->
            <view class="offerItem-wrapper" bindtap='gotoIntegral'>
                <view class='offer-num'> {{integral}} </view>
                <view class='offer-title'> 积分{{windowHeight}}- {{scrollTop}} </view>
            </view>
        </view>
    </view>
    <view class="c_scrollPosition_forCalculate">
        <scroll-view scroll-y style="height: {{windowHeight - 150}}px;">
            <view wx:if='{{(userInfo.userType=="ORG_ADMIN"||userInfo.userType=="ADMIN")&&userInfo.orgAdmin==true}}'>
                <view class='org-admin'>
                    <view class='org-addfood' bindtap="gotoAddfoodAdmin">
                        <image src="/images/mine/data_meal.png" class='org-addfood'></image>
                    </view>
                    <view class='org-addfood' bindtap="gotoCabinetminiProgram">
                        <image src="/images/mine/data_cabinet.png" class='org-addfood'></image>
                    </view>
                </view>
                <view style='width:100%;height:20rpx;background: #F5F5F5;'></view>
            </view>

            <view class='add-food' bindtap="gotoAddfood" style='border-bottom:2rpx solid #F5F5F5;'>
                <view style='width:44rpx;height:44rpx;'>
                    <image src="/images/mine/me_pic.png" style='width:44rpx;height:44rpx;'></image>
                </view>
                <view class='add-title'>我要补餐</view>
                <image src="/images/mine/xiangyou.png" style='width:34rpx;height:34rpx;'></image>
            </view>
            <view class='add-food' bindtap="gotoSaySomething" style='border-bottom:2rpx solid #F5F5F5;'>
                <view style='width:44rpx;height:44rpx;'>
                    <image src="/images/mine/me_complaint.png" style='width:44rpx;height:44rpx;'></image>
                </view>
                <view class='add-title'>我要吐槽</view>
                <image src="/images/mine/xiangyou.png" style='width:34rpx;height:34rpx;'></image>
            </view>
            <view class='add-food' bindtap="gotoEvaluateService" wx:if='{{(userInfo.orgAdmin==true)&&(userInfo.userType=="ORG_ADMIN"||userInfo.userType=="ADMIN")}}'>
                <view style='width:44rpx;height:44rpx;'>
                    <image src="/images/mine/service.png" style='width:44rpx;height:44rpx;'></image>
                </view>
                <view class='add-title'>我要对服务评分</view>
                <image src="/images/mine/xiangyou.png" style='width:34rpx;height:34rpx;'></image>
            </view>
            <view style='background: #F5F5F5;width:100%;height:20rpx;'></view>
            <view class='mine-service'>我的服务</view>
            <view class='service-list'>
                <view wx:for="{{labelList}}" wx:for-index="labelIndex" wx:for-item="labelItem" wx:key="labelIndex" bindtap="handleClickLabel" data-labelindex="{{labelIndex}}" data-labelitem="{{labelItem}}">
                    <view style="margin-left:12rpx;width:88rpx;height:88rpx;margin-bottom:10rpx;">
                        <image src="/images/mine/{{imageList[labelIndex]}}.png" style='width:88rpx;height:88rpx;'></image>
                    </view>
                    <view class='label-title'>{{labelItem}}</view>
                </view>
            </view>
            <!-- <view class='service-list'>
                <view bindtap="gotoSaySomething">
                    <view style="margin-left:12rpx;width:88rpx;height:88rpx;margin-bottom:10rpx;">
                        <image src="/images/mine/me_complaint.png" style='width:88rpx;height:88rpx;'></image>
                    </view>
                    <view class='label-title'>我要吐槽</view>
                </view>
            </view> -->
        </scroll-view>
    </view>
</view>