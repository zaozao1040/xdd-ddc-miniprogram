<view class="discount-wrapper">


    <view class="content">
        <view class="content-header">
            <view class="content-header-item {{itemStatusActiveFlag=='weishiyong'?'content-header-item-active':''}}" catchtap="changeItemStatusActiveFlag" data-flag="weishiyong">
                未使用(0)
            </view>
            <view class="content-header-item {{itemStatusActiveFlag=='yishiyong'?'content-header-item-active':''}}" catchtap="changeItemStatusActiveFlag" data-flag="yishiyong">
                已使用(0)
            </view>
            <view class="content-header-item {{itemStatusActiveFlag=='yiguoqi'?'content-header-item-active':''}}" catchtap="changeItemStatusActiveFlag" data-flag="yiguoqi">
                已过期(0)
            </view>
        </view>
    </view>
    <view class="c_scrollPosition_forCalculate">

        <view class="noResult" style="font-size:30rpx;color:#666;text-align: center;height: 80rpx;line-height: 80rpx;">
            未查询到优惠券记录..
        </view>
    </view>
</view>