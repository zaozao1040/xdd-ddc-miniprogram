 .comment-label {
     display: flex;
     flex-wrap: wrap;
 }
 
 .flex-a {
     display: flex;
 }
 
 .foodItem {
     padding: 20rpx 30rpx 30rpx;
     background: #fff;
     margin-top: 16rpx;
     border-radius: 6rpx;
 }
 
 .foodName {
     font-size: 32rpx;
     font-family: PingFang-SC-Medium;
     font-weight: 500;
     color: rgba(51, 51, 51, 1);
     line-height: 56rpx;
     margin-left: 20rpx;
 }
 
 .foodDes {
     font-family: PingFangSC-Regular;
     font-size: 24rpx;
     color: #999999;
 }
 
 .foodPrice {
     color: #FF7800;
 }
 
 .welcome {
     font-family: PingFangSC-Regular;
     font-size: 30rpx;
     color: #666666;
     height: 42rpx;
     margin-top: 30rpx;
     padding: 30rpx 0 20rpx 0;
     text-align: center;
     border-top: #F5F5F5 solid 2rpx;
 }
 
 .labelItem {
     background: #fff;
     border-radius: 30rpx;
     height: 52rpx;
     line-height: 52rpx;
     width: 176rpx;
     font-family: PingFangSC-Regular;
     font-size: 24rpx;
     color: #999999;
     text-align: center;
     border: 2rpx solid #EBEBEB;
     margin-top: 20rpx;
 }
 
 .labelItem-active {
     background-image: linear-gradient(270deg, #F79C4C 0%, #F79C4C 100%);
     border-radius: 30rpx;
     height: 56rpx;
     line-height: 56rpx;
     width: 180rpx;
     font-family: PingFangSC-Regular;
     font-size: 24rpx;
     color: #FFFFFF;
     text-align: center;
     margin-top: 20rpx;
 }
 
 .yingjifen {
     font-family: PingFangSC-Regular;
     font-size: 26rpx;
     color: #F79C4C;
     margin-top: 120rpx;
 }
 /* 上传图片 */
 
 .img-wrapper {
     display: flex;
     width: 100%;
 }
 
 .image {
     margin-right: 10rpx;
     height: 130rpx;
     width: 130rpx;
     color: #999;
     background: #F9F9F9;
     border-radius: 3px;
     margin-top: 44rpx;
 }
 
 .image-remove {
     line-height: 24rpx;
     text-align: center;
     font-size: 20rpx;
     background: #333;
     width: 30rpx;
     height: 28rpx;
     position: absolute;
     color: #fff;
     border-radius: 50%;
     padding-bottom: 2rpx;
     z-index: 500;
     margin-left: 110rpx;
     margin-top: 30rpx;
 }
 
 .image-shangchuan {
     margin-right: 10rpx;
     height: 130rpx;
     width: 130rpx;
     margin-top: 44rpx;
 }
 
 .button {
     background-image: linear-gradient(270deg, #F79C4C 0%, #F7CD4E 99%);
     border-radius: 50rpx;
     font-family: PingFangSC-Regular;
     font-size: 34rpx;
     color: #FFFFFF;
     text-align: center;
     height: 96rpx;
     line-height: 96rpx;
     bottom: 30rpx;
     left: 0;
     right: 0;
     width: 630rpx;
     margin: auto;
     position: fixed;
 }
 
 .success-1 {
     margin-top: 40rpx;
     font-size: 40rpx;
     font-family: Helvetica;
     color: rgba(51, 51, 51, 1);
     line-height: 48rpx;
 }
 
 .success-2 {
     margin-top: 6rpx;
     font-size: 28rpx;
     font-family: PingFang-SC-Medium;
     font-weight: 500;
     color: rgba(247, 156, 76, 1);
     line-height: 40rpx;
 }
 
 .success-3 {
     margin-top: 18rpx;
     font-size: 30rpx;
     font-family: PingFang-SC-Regular;
     font-weight: 400;
     color: rgba(102, 102, 102, 1);
     line-height: 42rpx;
 }
 
 .top {
     background: rgba(242, 153, 74, 0.15);
     border-radius: 6rpx;
     position: sticky;
     top: 0;
     margin: 20rpx 24rpx;
     font-size: 28rpx;
     font-family: PingFang-SC-Regular;
     font-weight: 400;
     color: rgba(102, 102, 102, 1);
     line-height: 80rpx;
     padding-left: 30rpx;
     width: 672rpx;
 }
 
 .service-wrapper {
     background: rgba(255, 255, 255, 1);
     border-radius: 6rpx;
     padding: 30rpx;
 }
 
 .service-title {
     font-size: 30rpx;
     font-family: PingFang-SC-Regular;
     font-weight: 400;
     color: rgba(77, 77, 77, 1);
     line-height: 42rpx;
     margin-bottom: 10rpx;
 }
 
 .service-title-des {
     font-size: 22rpx;
     font-family: PingFang-SC-Regular;
     font-weight: 400;
     color: rgba(242, 153, 74, 1);
     line-height: 32rpx;
     margin: 6rpx 0 0 10rpx;
 }
 
 .service-des {
     font-size: 22rpx;
     font-family: PingFang-SC-Regular;
     font-weight: 400;
     color: rgba(153, 153, 153, 1);
     line-height: 32rpx;
     margin-bottom: 20rpx;
 }
 
 .textarea {
     width: 588rpx;
     height: 80rpx;
     font-size: 28rpx;
     font-family: PingFangSC-Regular;
     font-weight: 400;
     color: rgba(203, 203, 203, 1);
     line-height: 40rpx;
 }